<view class="My_center_body"><view class="my_center_none"><view class="center_none_all"><block wx:if="{{userData===null||userData===undefined}}"><image class="none_tou_xiang" src="../../static/picture/touxiang.jpeg" mode></image></block><block wx:else><image class="none_tou_xiang" src="{{userData.faceImage}}" mode></image></block><block wx:if="{{userData===null||userData===undefined}}"><view data-event-opts="{{[['tap',[['jumpLogin',['$event']]]]]}}" class="none_content" bindtap="__e">登录/注册</view></block><block wx:else><view class="my_center_nickname"><view>{{"昵称："+userData.username}}</view><view>{{"ID："+userData.id}}</view></view></block></view></view><block wx:if="{{userData!==null&&userData!==undefined}}"><view class="my_center_existence"><view class="my_center_all"><view class="my_center_nickname">{{'昵称：'+userData.username+''}}</view><view class="my_center_modify"><button class="center_modify" type="primary" data-event-opts="{{[['tap',[['modifyObj',['$event']]]]]}}" bindtap="__e">修改个人资料</button></view></view></view></block><view hidden="{{!(userData!==null&&userData!==undefined)}}" class="my_center_out"><button class="center_out_btn" type="warn" data-event-opts="{{[['tap',[['loginOut',['$event']]]]]}}" bindtap="__e">退出登录</button></view></view>