.details_first {
	position: relative;
}
.first_return,
.first_share {
	width: 60rpx;
	height: 60rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	background-color: #515151;
	border-radius: 50%;
}
.first_return {
	position: absolute;
	top: 10rpx;
	left: 10rpx;
	z-index: 1000;
}
.first_share {
	position: absolute;
	top: 10rpx;
	right: 10rpx;
	z-index: 1000;
}
.first_return_img {
	width: 40rpx;
	height: 40rpx;
	margin-left: 50%;
	-webkit-transform: translateX(-20rpx);
	    -ms-transform: translateX(-20rpx);
	        transform: translateX(-20rpx)
}
.first_share_img {
	width: 50rpx;
	height: 40rpx;
	margin-left: 50%;
	-webkit-transform: translateX(-15rpx);
	    -ms-transform: translateX(-15rpx);
	        transform: translateX(-15rpx)
}
